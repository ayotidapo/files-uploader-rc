{
  "name": "files-uploader-rc",
  "version": "3.2.5",
  "description": "Made with create-react-library",
  "author": "https://github.com/ayotidapo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayotidapo/files-uploader-rc.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16 || ^17",
    "react-dom": "^15.0.0 || ^16 || ^17",
    "react-scripts": "^3.4.1"
  },
  "dependencies": {
    "rc-progress": "^3.1.3",
    "sass": "^1.57.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "axios": "^0.21.4",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rc-progress": "^3.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "uuid": "^8.3.2"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/ayotidapo/files-uploader-rc/issues"
  },
  "homepage": "https://github.com/ayotidapo/files-uploader-rc#readme",
  "directories": {
    "example": "example"
  },
  "eslintConfig": {
    "globals": {
      "window": true
    }
  }
}
